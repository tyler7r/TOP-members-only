<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <% if (member === true) { %>
        <ul>
            <% for (let i = 0; i < messages.length; i++ ) { %>
                <div>
                    <%= messages[i].author.fullname %>
                    <a href='<%= messages[i].url %>'><%= messages[i].title %></a>
                    <%= messages[i].message %>
                <div>
    
            <% } %>
        </ul>
    <% } else { %>
        <ul>
            <% for (let i = 0; i < messages.length; i++ ) { %>
                <div>
                    <a href='<%= messages[i].url %>'><%= messages[i].title %></a>
                    <%= messages[i].message %>
                <div>
    
            <% } %>
        </ul>
    <% } %>
  </body>
</html>